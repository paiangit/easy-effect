{
  "name": "easy-effect",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^4.18.9",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.8",
    "file-saver": "^2.0.5",
    "jszip": "^3.7.1",
    "lodash-es": "^4.17.21",
    "lottie-web": "^5.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-window": "^1.8.6",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "craco start",
    "start:measure": "npx cross-env MEASURE=true craco start",
    "build": "craco build",
    "build:analyze": "npx cross-env ANALYZE=true craco build",
    "test": "craco test --watchAll=false --forceExit",
    "test:coverage": "craco test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "predeploy": "pnpm build",
    "deploy": "pnpm predeploy && gh-pages -d build -r git@github.com:paiangit/paiangit.github.io.git -b main"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@craco/craco": "^6.4.3",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "circular-dependency-plugin": "^5.2.2",
    "craco-alias": "^3.0.1",
    "craco-less": "^2.0.0",
    "cross-env": "^7.0.3",
    "esbuild-loader": "^2.18.0",
    "eslint-config-prettier": "^8.5.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "postcss-less": "^6.0.0",
    "prettier": "^2.5.1",
    "speed-measure-webpack-plugin": "^1.5.0",
    "stylelint": "^14.5.3",
    "stylelint-config-standard": "^25.0.0",
    "thread-loader": "^3.0.4",
    "typescript": "^4.5.5",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix --cache",
      "git add"
    ],
    "*.less": [
      "stylelint --fix --cache --custom-syntax postcss-less",
      "git add"
    ],
    "*.{tsx,ts,js}": [
      "eslint --fix --cache",
      "prettier --write",
      "git add"
    ]
  }
}
